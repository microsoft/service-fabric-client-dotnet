<?xml version="1.0" encoding="utf-8"?>
<Configuration>
    <ViewDefinitions>
        <View>
            <Name>ApplicationHealth</Name>
            <ViewSelectedBy>
                <TypeName>Microsoft.ServiceFabric.Common.ApplicationHealth</TypeName>
            </ViewSelectedBy>
            <ListControl>
                <ListEntries>
                    <ListEntry>
                        <ListItems>
                            <ListItem>
                                <PropertyName>ApplicationName</PropertyName>
                                <FormatString>Name</FormatString>
                            </ListItem>
                            <ListItem>
                                <PropertyName>AggregatedHealthState</PropertyName>
                            </ListItem>
                            <ListItem>
                                <PropertyName>UnhealthyEvaluations</PropertyName>
                                <ItemSelectionCondition>
                                    <ScriptBlock>$_.UnhealthyEvaluations.Count -gt 0</ScriptBlock>
                                </ItemSelectionCondition>
                                <FormatString>{0}</FormatString>
                            </ListItem>
                            <ListItem>
                                <PropertyName>ServiceHealthStates</PropertyName>
                                <FormatString>{0}</FormatString>
                            </ListItem>
                            <ListItem>
                                <PropertyName>DeployedApplicationHealthStates</PropertyName>
                                <FormatString>{0}</FormatString>
                            </ListItem>
                            <ListItem>
                                <PropertyName>HealthEvents</PropertyName>
                                <FormatString>{0}</FormatString>
                            </ListItem>
                            <ListItem>
                                <PropertyName>HealthStatistics</PropertyName>
                                <ItemSelectionCondition>
                                    <ScriptBlock>($_.HealthStatistics -ne $null) -and ($_.HealthStatistics.HealthStateCountList.Count -ge 1)</ScriptBlock>
                                </ItemSelectionCondition>
                                <FormatString>{0}</FormatString>
                            </ListItem>
                        </ListItems>
                    </ListEntry>
                </ListEntries>
            </ListControl>
        </View>
        <View>
            <Name>ApplicationTypeManifest</Name>
            <ViewSelectedBy>
                <TypeName>Microsoft.ServiceFabric.Common.ApplicationTypeManifest</TypeName>
            </ViewSelectedBy>
            <ListControl>
                <ListEntries>
                    <ListEntry>
                        <ListItems>
                            <ListItem>
                                <PropertyName>Manifest</PropertyName>
                                <FormatString>Manifest</FormatString>
                            </ListItem>
                        </ListItems>
                    </ListEntry>
                </ListEntries>
            </ListControl>
        </View>
        <View>
            <Name>ClusterHealth</Name>
            <ViewSelectedBy>
                <TypeName>Microsoft.ServiceFabric.Common.ClusterHealth</TypeName>
            </ViewSelectedBy>
            <ListControl>
                <ListEntries>
                    <ListEntry>
                        <ListItems>
                            <ListItem>
                                <PropertyName>AggregatedHealthState</PropertyName>
                            </ListItem>
                            <ListItem>
                                <PropertyName>NodeHealthStates</PropertyName>
                                <ItemSelectionCondition>
                                    <ScriptBlock>$_.NodeHealthStates.Count -gt 0</ScriptBlock>
                                </ItemSelectionCondition>
                                <FormatString>{0}</FormatString>
                            </ListItem>
                            <ListItem>
                                <PropertyName>ApplicationHealthStates</PropertyName>
                                <ItemSelectionCondition>
                                    <ScriptBlock>$_.ApplicationHealthState.Count -gt 0</ScriptBlock>
                                </ItemSelectionCondition>
                                <FormatString>{0}</FormatString>
                            </ListItem>
                            <ListItem>
                                <PropertyName>HealthEvents</PropertyName>
                                <FormatString>{0}</FormatString>
                            </ListItem>
                            <ListItem>
                                <PropertyName>HealthStatistics</PropertyName>
                                <ItemSelectionCondition>
                                    <ScriptBlock>($_.HealthStatistics -ne $null) -and ($_.HealthStatistics.HealthStateCountList.Count -ge 1)</ScriptBlock>
                                </ItemSelectionCondition>
                                <FormatString>{0}</FormatString>
                            </ListItem>
                        </ListItems>
                    </ListEntry>
                </ListEntries>
            </ListControl>
        </View>
        <View>
            <Name>ClusterManifest</Name>
            <ViewSelectedBy>
                <TypeName>Microsoft.ServiceFabric.Common.ClusterManifest</TypeName>
            </ViewSelectedBy>
            <ListControl>
                <ListEntries>
                    <ListEntry>
                        <ListItems>
                            <ListItem>
                                <PropertyName>Manifest</PropertyName>
                                <FormatString>Manifest</FormatString>
                            </ListItem>
                        </ListItems>
                    </ListEntry>
                </ListEntries>
            </ListControl>
        </View>
        <View>
            <Name>ClusterVersion</Name>
            <ViewSelectedBy>
                <TypeName>Microsoft.ServiceFabric.Common.ClusterVersion</TypeName>
            </ViewSelectedBy>
            <ListControl>
                <ListEntries>
                    <ListEntry>
                        <ListItems>
                            <ListItem>
                                <PropertyName>properties</PropertyName>
                                <FormatString>properties</FormatString>
                            </ListItem>
                        </ListItems>
                    </ListEntry>
                </ListEntries>
            </ListControl>
        </View>
        <View>
            <Name>ServiceHealth</Name>
            <ViewSelectedBy>
                <TypeName>Microsoft.ServiceFabric.Common.ServiceHealth</TypeName>
            </ViewSelectedBy>
            <ListControl>
                <ListEntries>
                    <ListEntry>
                        <ListItems>
                            <ListItem>
                                <PropertyName>HealthEvents</PropertyName>
                                <FormatString>{0}</FormatString>
                            </ListItem>
                            <ListItem>
                                <PropertyName>HealthStatistics</PropertyName>
                                <ItemSelectionCondition>
                                    <ScriptBlock>($_.HealthStatistics -ne $null) -and ($_.HealthStatistics.HealthStateCountList.Count -ge 1)</ScriptBlock>
                                </ItemSelectionCondition>
                                <FormatString>{0}</FormatString>
                            </ListItem>
                        </ListItems>
                    </ListEntry>
                </ListEntries>
            </ListControl>
        </View>
        <View>
            <Name>ServiceTypeManifest</Name>
            <ViewSelectedBy>
                <TypeName>Microsoft.ServiceFabric.Common.ServiceTypeManifest</TypeName>
            </ViewSelectedBy>
            <ListControl>
                <ListEntries>
                    <ListEntry>
                        <ListItems>
                            <ListItem>
                                <PropertyName>Manifest</PropertyName>
                                <FormatString>Manifest</FormatString>
                            </ListItem>
                        </ListItems>
                    </ListEntry>
                </ListEntries>
            </ListControl>
        </View>
    </ViewDefinitions>
</Configuration>
